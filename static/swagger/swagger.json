          "用户收货地址"
        ],
        "summary": "添加收货地址",
        "parameters": [
          {
            "type": "string",
            "description": "收货人",
            "name": "name",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "手机号",
            "name": "mobile",
            "in": "formData",
            "required": true
          },
          {
            "type": "integer",
            "description": "省",
            "name": "code_prov",
            "in": "formData",
            "required": true
          },
          {
            "type": "integer",
            "description": "市",
            "name": "code_city",
            "in": "formData",
            "required": true
          },
          {
            "type": "integer",
            "description": "区",
            "name": "code_coun",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "详情地址",
            "name": "address",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "门牌号，楼房号",
            "name": "room_number",
            "in": "formData",
            "required": true
          },
          {
            "type": "string",
            "description": "是否默认(false|true)",
            "name": "is_default",
            "in": "formData",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "成功",
            "schema": {
              "$ref": "#/definitions/resp"
            }
          }
        }
      }
    },
